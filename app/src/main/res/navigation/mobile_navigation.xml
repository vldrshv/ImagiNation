<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_gallery">

    <fragment
        android:id="@+id/navigation_gallery"
        android:name="com.example.imagination.core.view.activities.ui.gallery.GalleryFragment"
        android:label="@string/title_gallery"
        tools:layout="@layout/fragment_gallery">
    </fragment>

    <fragment
        android:id="@+id/navigation_liked_photos"
        android:name="com.example.imagination.core.view.activities.ui.liked_photos.LikedGalleryFragment"
        android:label="@string/title_liked_photos"
        tools:layout="@layout/fragment_liked_gallery" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.imagination.core.view.activities.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />
</navigation>